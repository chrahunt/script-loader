{
	"manifest_version": 2,
	"name": "Local Loader",
	"description": "Load local files for injection into website.",
	"version": "0.1",
	"permissions": [
		"storage"
	],
	"icons": { 
		"16": "img/icon16.png",
		"48": "img/icon48.png",
		"128": "img/icon128.png" 
	},
	"browser_action": {
		"default_icon": {                    
			"19": "img/icon48.png",          
			"38": "img/icon48.png"          
		},
		"default_popup": "popup.html"
	},
	"web_accessible_resources": [
		"img/icon128.png"
	],
	"content_scripts": [
    {
      "matches": [
        "http://*.koalabeast.com/*",
        "http://*.newcompte.fr/*"
      ],
      "js": [
        "page-injector.js"
      ],
      "run_at": "document_end"
    }
  ]
}
